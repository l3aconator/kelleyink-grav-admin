                    <p class="work-post-key-feature-body">{{ page.header.highlightcontenttwo }}</p>
                    
                    {% if page.header.hightlightbuttontwo == true %}
                        <a class="um-center" href="{{ page.header.highlightbuttontwolink }}"><button class="button-cream um-btn-full-width">{{ page.header.hightlightbuttoncontenttwo }}</button></a>
                    {% else %}
                    {{ page.header.highlightbuttontwooutsidelink }}
                    {% endif %}
                  
                    {% if page.header.hightlightbuttonoutsidetwo == true %}
                        <a class="um-center" href="{{ page.header.highlightbuttonoutsidetwolink }}" target="_blank"><button class="button-cream um-marg-bot-44 um-btn-full-width">{{ page.header.hightlightbuttonoutsidecontenttwo }}</button></a>
                    {% else %}
                    {% endif %}
                        
                </div>
            </div>
            <div class="row container work-post-feature-padding case-study-col-break">
                <div class="three columns um-marg-bot-22">
                   <div class="work-post-key-feature-wrap">
                       <div class="work-post-feature-name-wrap">
                           <div class="work-post-key-feature-heading">{{ page.header.highlightheadingthree }}</div>
