{% embed 'partials/base.html.twig' %}

{% block content %}
    <section class="work-post-bg black-gradient-bg section-pad">
            <div class="container">
                <h1 class="work-post-heading">{{ page.header.title }}</h1>
              
                {% if page.header.topbottomimageinside == true %}
                  <a href="{{ page.header.topbottomimagelink }}">
                {% endif %}
                    
                {% if page.header.topbottomimageoutside == true %}
                  <a href="{{ page.header.topbottomimageoutsidelink }}" target="_blank">
                {% endif %}
                    
                      <div class="work-post-header-image-wrap case-study">
                          <img class="work-post-header-image case-study bottom" src="{{ page.media[page.header.bottomimage].url }}" alt="" />
                          <img class="work-post-header-image case-study top" src="{{ page.media[page.header.topimage].url }}" alt="" />
                      </div>
                  </a>
            </div>
        </section>
        <section class="work-project-overview-bg cream-bg section-pad">
            <div class="container">
            {{ page.content }}
            </div>
        </section>
        <section class="work-post-key-bg blue-gradient-bg section-pad">
            <h2 class="work-post-feature-heading">{{ page.header.highlightmainheading }}</h2>
            <div class="row container work-post-feature-padding case-study-col-break">
                <div class="three columns um-marg-bot-22">
                   <div class="work-post-key-feature-wrap">
                       <div class="work-post-feature-name-wrap">
                           <div class="work-post-key-feature-heading">{{ page.header.highlightheadingone }}</div>
                       </div>
                        <img class="work-post-key-feature-image" src="{{ page.media[page.header.highlightimageone].url }}" alt="" />
                    </div>
                </div>
                <div class="three columns">
                    <p class="work-post-key-feature-body">{{ page.header.highlightcontentone }}</p>
                    
                    {% if page.header.hightlightbuttonone == true %}
                        <a class="um-center" href="{{ page.header.highlightbuttononelink }}"><button class="button-cream um-marg-bot-44 um-btn-full-width">{{ page.header.hightlightbuttoncontentone }}</button></a>
                    {% else %}
                    {% endif %}
                  
                    {% if page.header.hightlightbuttonoutsideone == true %}
                        <a class="um-center" href="{{ page.header.highlightbuttonoutsideonelink }}" target="_blank"><button class="button-cream um-marg-bot-44 um-btn-full-width">{{ page.header.hightlightbuttonoutsidecontentone }}</button></a>
                    {% else %}
                    {% endif %}
                    
                </div>
                <div class="three columns um-marg-bot-22">
                   <div class="work-post-key-feature-wrap">
                       <div class="work-post-feature-name-wrap">
                           <div class="work-post-key-feature-heading">{{ page.header.highlightheadingtwo }}</div>
                       </div>
                        <img class="work-post-key-feature-image" src="{{ page.media[page.header.highlightimagetwo].url }}" alt="" />
